/* Completely Working */

/* The original question can be found at
https://study.cs50.net/argv

*/

#include <iostream>

int main(int argc, char* argv[])
{
    int count = 0;
    for(int i = 1; i < argc; i++)
    {
        int k = 0;
        while( argv[i][k] != '\0')
        {
            count++;
            k++;
        }
    }
    
    std::cout << count << std::endl;
    return 0;
}
